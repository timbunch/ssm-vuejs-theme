{
  "name": "ssm-vue-theme",
  "version": "1.0.0",
  "description": "A streamlined hybrid front end theme for Wordpress and VueJS",
  "main": "index.js",
  "scripts": {
    "dev": "npm run development",
    "development": "NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "npm run development -- --watch",
    "hot": "NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
    "prod": "npm run production",
    "production": "npm update; NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js;zip -r ssm-vuejs-theme.zip ssm-vuejs-theme;"
  },
  "author": "simplysmartmedia.com",
  "license": "ISC",
  "devDependencies": {
    "@mdi/js": "^4.9.95",
    "css-loader": "^3.6.0",
    "laravel-mix": "^5.0.9",
    "resolve-url-loader": "^3.1.2",
    "sass": "^1.32.4",
    "sass-loader": "^9.0.3",
    "vue-template-compiler": "^2.6.12"
  },
  "dependencies": {
    "@mdi/font": "^4.9.95",
    "axios": "^0.21.1",
    "moment": "^2.29.1",
    "moment-round": "^1.0.1",
    "node-sass": "^4.14.1",
    "vue": "^2.6.12",
    "vue-axios": "^2.1.5",
    "vue-infinite-scroll": "^2.0.2",
    "vue-router": "^3.4.9",
    "vuetify": "^2.4.2",
    "vuetify-loader": "^1.6.0",
    "vuetifyjs-mix-extension": "0.0.11",
    "vuex": "^3.6.0"
  }
}
